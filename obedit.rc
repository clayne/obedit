        MENUITEM "Reset List State",            ID_VIEW_RESETLISTS
        MENUITEM SEPARATOR
        MENUITEM "Resource Viewer",             ID_VIEW_RESOURCEVIEWER
    END
    POPUP "&Window"
    BEGIN
        MENUITEM "&New Window",                 ID_WINDOW_NEW
        MENUITEM "&Cascade",                    ID_WINDOW_CASCADE
        MENUITEM "&Tile",                       ID_WINDOW_TILE_HORZ
        MENUITEM "&Arrange Icons",              ID_WINDOW_ARRANGE
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About obedit...",            ID_APP_ABOUT
        MENUITEM SEPARATOR
        MENUITEM "UESP Wiki",                   ID_HELP_UESPWIKI
        MENUITEM "Official CS Wiki",            ID_HELP_CSWIKI
        MENUITEM SEPARATOR
        MENUITEM "Test Field Lookup",           ID_HELP_TESTFIELDLOOKUP
        MENUITEM "Test Find FormID",            ID_HELP_TESTFINDFORMID
        MENUITEM "Test Memory Speed",           ID_TEST_MEMORYSPEED
        MENUITEM "Test Tokenize Scripts",       ID_TEST_TOKENSCRIPTS
        MENUITEM "Test Output Effects",         ID_HELP_TESTOUTPUTEFFECTS
